{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "beyondcode/laravel-websockets": "^1.14",
        "cviebrock/eloquent-sluggable": "^10.0",
        "f9webltd/laravel-api-response-helpers": "^1.5",
        "garygreen/pretty-routes": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "haruncpi/laravel-user-activity": "^1.0",
        "hisorange/browser-detect": "^4.5",
        "kutia-software-company/larafirebase": "^1.3",
        "laravel/framework": "^10.10",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravolt/avatar": "^5.0",
        "maatwebsite/excel": "^3.1",
        "opcodesio/log-viewer": "^2.5",
        "protonemedia/laravel-verify-new-email": "^1.8",
        "protoqol/prequel": "^1.23",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-permission": "^5.10",
        "vildanbina/laravel-model-json": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.21",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "laravel/telescope": "^4.15",
        "laravolt/indonesia": "^0.34.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
